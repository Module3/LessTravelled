<div class='form-div'>
  <form class='form'>
    
    <input id="from" class='input' type='text' onfocus="if(this.value == 'Start:') { this.value = ''; }" value="Start:" onblur="if(this.value == '') {this.value='Start:';}"></input>
    
    <input id="to" class='input' type='text' onfocus="if(this.value == 'End:') { this.value = ''; }" value="End:" onblur="if(this.value == '') {this.value='End:';}"></input>
    
    <input id='search-term' class='input' type='text' onfocus="if(this.value == 'Find:') { this.value = ''; }" value="Find:" onblur="(this.value == '') ? this.value='Find:' : this.style='{color: black;}'"></input>
    
    <div id="submit" class='submit'><a href='#search'>SEARCH</a></div>
    
    <p class='advanced-button'>Advanced</p>
  
  </form>
</div>